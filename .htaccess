RewriteEngine on

# If not a directory, remove the trailing slash
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^([^/]+)/$ $1 [R=301,L]

# this will setup all redirects
RewriteCond %{REQUEST_URI} !^(index\.html|robots\.txt|img|font|js|css) [NC]

#api
RewriteRule ^.*life/([a-zA-Z0-9+-]+)*/*$ /multisplode/index.html?apiTarget=$1 [L,NC,QSA]

#main dashboard
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^?]*)$ /multisplode/index.html?page=$1 [NC,L,QSA]